{
  "format": "page",
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "Uid",
          "placeholder": "UID"
        }
      },
      "enable_site": {
        "type": "Boolean",
        "config": {
          "label": "Ouvrir le site",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Titre",
          "placeholder": "Titre"
        }
      },
      "subtitle": {
        "type": "Text",
        "config": {
          "label": "Sous-titre",
          "placeholder": "Sous-titre"
        }
      },
      "logo": {
        "type": "Image",
        "config": {
          "label": "Logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "date_start": {
        "type": "Date",
        "config": {
          "label": "Date de début",
          "placeholder": "Date de début"
        }
      },
      "date_end": {
        "type": "Date",
        "config": {
          "label": "Date de fin",
          "placeholder": "Date de fin"
        }
      },
      "subscribe_link": {
        "type": "Link",
        "config": {
          "label": "Lien inscription",
          "placeholder": "Lien d'inscription",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Texte de présentation",
          "placeholder": "Présentation",
          "allowTargetBlank": false,
          "multi": "paragraph,heading3,heading4,heading5,heading6,strong,em,hyperlink,list-item,o-list-item,rtl"
        }
      },
      "image_description": {
        "type": "Image",
        "config": {
          "label": "Imagede  présentation",
          "constraint": {},
          "thumbnails": []
        }
      },
      "place_name": {
        "type": "Text",
        "config": {
          "label": "Nom du lieu",
          "placeholder": "Nom du lieu"
        }
      },
      "place_coords": {
        "type": "GeoPoint",
        "config": {
          "label": "Coordonnées géographiques"
        }
      },
      "gallery": {
        "type": "Group",
        "config": {
          "label": "Galerie",
          "repeat": true,
          "fields": {
            "image": {
              "type": "Image",
              "config": {
                "label": "Image",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
